{
  "name": "backend_template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:unit": "jest --coverage --testPathPattern=test/unit/.*\\.test\\.ts$",
    "test:integration": "jest --testPathPattern=test/integration/.*\\.test\\.ts$ --testTimeout=30000",
    "build": "rm -rf dist/ && tsc",
    "tsc": "tsc",
    "dev": "nodemon dist/app.js",
    "start": "node dist/app.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx}'",
    "format-all": "npm run format && npm run lint"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.1",
    "jest": "^29.7.0",
    "jest-util": "^30.0.5",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.9",
    "prettier": "3.6.2",
    "semantic-release": "24.2.6",
    "ts-jest": "^29.4.1",
    "ts-node": "10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.31.0"
  },
  "dependencies": {
    "@sentry/node": "^8.46.0",
    "@sentry/profiling-node": "^8.46.0",
    "better-sqlite3": "^8.4.0",
    "body-parser": "^1.20.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "morgan": "^1.10.0"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
